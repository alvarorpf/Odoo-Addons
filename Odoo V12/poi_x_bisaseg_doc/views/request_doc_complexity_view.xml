<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.ui.view" id="request_doc_complexity_form">
        <field name="name">request.doc.complexity.form</field>
        <field name="model">request.doc.complexity</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model="ir.ui.view" id="request_doc_complexity_tree">
        <field name="name">request.doc.complexity.tree</field>
        <field name="model">request.doc.complexity</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_request_doc_complexity">
        <field name="name">Complejidad</field>
        <field name="res_model">request.doc.complexity</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>