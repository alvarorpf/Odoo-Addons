<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_reconcile_seq" model="ir.sequence">
            <field name="name">Secuencia Archivo Bizagi</field>
            <field name="code">poi.bizagi.import</field>
            <field name="prefix">biz</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
        <record id="sequence_request_document" model="ir.sequence">
            <field name="name">Secuencia Requerimiento de Documentos</field>
            <field name="code">request.document</field>
            <field name="prefix">REQ/%(range_month)s/%(range_year)s/</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>