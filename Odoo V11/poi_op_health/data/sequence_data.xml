<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="True">
        <record id="sequence_medical_file" model="ir.sequence">
            <field name="name">Sequence Medical File</field>
            <field name="code">sequence.medical.file</field>
            <field name="prefix">FM/%(year)s/</field>
            <field name="padding">3</field>
        </record>
        <record id="sequence_clinical_record" model="ir.sequence">
            <field name="name">Sequence Clinical Record</field>
            <field name="code">sequence.clinical.record</field>
            <field name="prefix">RCli/%(year)s/</field>
            <field name="padding">5</field>
        </record>
        <record id="sequence_exit_authorization" model="ir.sequence">
            <field name="name">Sequence Exit Authorization</field>
            <field name="code">sequence.exit.authorization</field>
            <field name="prefix">AutS/%(year)s/</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>