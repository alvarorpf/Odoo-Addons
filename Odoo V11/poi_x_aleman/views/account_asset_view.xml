<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
         <record id="account_asset_inherit_aleman" model="ir.ui.view">
            <field name="name">account.asset.inherit.aleman</field>
            <field name="model">account.asset.asset</field>
            <field name="inherit_id" ref="poi_bol_asset.view_account_asset_bol_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='serial']" position="after">
                    <field name="amount"/>
                    <field name="notes"/>
                </xpath>
            </field>
         </record>
    </data>
</odoo>